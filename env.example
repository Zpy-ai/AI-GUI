# Qwen API (阿里云 DashScope 兼容模式或其他服务商的 OpenAI 兼容端点)
QWEN_API_KEY=your_qwen_api_key_here
# 推荐 DashScope 兼容模式端点：
# https://dashscope.aliyuncs.com/compatible-mode/v1
# 或者其他服务商（示例：SiliconFlow）端点：
# https://api.siliconflow.cn/v1
QWEN_API_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1
# 可选：如果服务商的模型名与前端选择的不一致，可以用此项强制覆盖
# 例如：qwen-plus / qwen-turbo / qwen2.5-72b-instruct 等
QWEN_MODEL_ID=

# Doubao (豆包) OpenAI 兼容接口
DOUBAO_API_KEY=your_doubao_api_key_here
# 示例端点（请以服务商控制台为准）：
# https://ark.cn-beijing.volces.com/api/v3 (非兼容)；如走兼容需使用服务商提供的 OAI 兼容地址
DOUBAO_API_ENDPOINT=https://ark.cn-beijing.volces.com/api/v3
DOUBAO_MODEL_ID=Doubao-Seed-1.6-vision

# Hunyuan (腾讯混元) OpenAI 兼容接口
HUNYUAN_API_KEY=your_hunyuan_api_key_here
# 示例端点（请以服务商控制台为准）：
HUNYUAN_API_ENDPOINT=
HUNYUAN_MODEL_ID=hunyuan-pro

# DeepSeek OpenAI 兼容接口
DEEPSEEK_API_KEY=your_deepseek_api_key_here
# 官方： https://api.deepseek.com/v1
DEEPSEEK_API_ENDPOINT=https://api.deepseek.com/v1
DEEPSEEK_MODEL_ID=deepseek-chat

# Kimi (Moonshot) OpenAI 兼容接口
KIMI_API_KEY=your_kimi_api_key_here
# 官方： https://api.moonshot.cn/v1
KIMI_API_ENDPOINT=https://api.moonshot.cn/v1
KIMI_MODEL_ID=kimi-k2-0711-preview

# 默认配置
DEFAULT_MODEL=kimi-k2
MAX_TOKENS=1000
TEMPERATURE=0.7

# MySQL 数据库配置
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=你的MySQL密码
DB_DATABASE=aiweb_database
DB_PORT=3306


# 应用配置
NEXT_PUBLIC_APP_NAME=AI助手
NEXT_PUBLIC_APP_VERSION=1.0.0